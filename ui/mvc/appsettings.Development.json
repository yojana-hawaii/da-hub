{
  "ConnectionStrings-sample": {
    "DaHub-sample": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=DaHubDirectory;Integrated Security=True;"
  },
  "Federation-sample": {
    "address": "https://{federation.example.com}/adfs/.well-known/openid-configuration",
    "clientId": "{client-id-from-adfs}"
  },
  "Okta-sample": {
    "ClientId": "{client-id-from-okta}",
    "ClientSecret": "{client-secret-from-okta}",
    "Domain": "https://{dev-acct-or-org}.okta.com",
    "PostLogoutRedirectUri": "https://localhost:7293/"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
